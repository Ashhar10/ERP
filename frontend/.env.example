# ================================================
# FRONTEND ENVIRONMENT VARIABLES
# ================================================
# Copy this file to .env and update with your actual values
# Location: /frontend/.env

# Supabase Configuration (from Supabase Dashboard → Settings → API)
# IMPORTANT: Use the 'anon/public' key, NOT the service_role key!
REACT_APP_SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
REACT_APP_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_anon_key_here

# Backend API URL
# Development
REACT_APP_API_URL=http://localhost:5000

# Production (uncomment when deploying)
# REACT_APP_API_URL=https://your-backend-render-url.onrender.com

# Optional: Enable/Disable features
# REACT_APP_ENABLE_ANALYTICS=false
# REACT_APP_ENABLE_DEBUG=true
